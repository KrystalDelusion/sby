[tasks]
prove_oss prove oss
noskip prove oss
cover_oss cover oss
prove_tabby prove tabby
cover_tabby cover tabby
prove_oss cover_oss : default

[options]
cover:
mode cover
--
prove:
mode prove
--

[engines]
cover: smtbmc z3
prove: abc pdr

[script]
read -sv fifo.sv
tabby: read -define USE_VERIFIC=1
noskip: read -define NOSKIP=1
read -formal top.sv
prep -top fifo

[files]
fifo.sv
top.sv
